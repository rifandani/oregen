{
  "name": "@rifandani/oregen",
  "version": "0.2.0",
  "description": "A Command Line Interface (CLI) for React to generate components, hooks, view model template, etc.",
  "author": "Tri Rizeki Rifandani",
  "homepage": "https://github.com/rifandani/oregen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rifandani/oregen.git"
  },
  "bugs": {
    "url": "https://github.com/rifandani/oregen/issues"
  },
  "keywords": [
    "oclif",
    "cli",
    "react"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "bin": {
    "oregen": "bin/run"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "version": "oclif readme && git add README.md",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@oclif/core": "^1.23.0",
    "@oclif/plugin-autocomplete": "^1.4.5",
    "@oclif/plugin-commands": "^2.2.2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.14",
    "@oclif/plugin-plugins": "^2.1.12",
    "cfonts": "^3.1.1",
    "cosmiconfig": "^8.0.0",
    "fs-extra": "^11.1.0",
    "lodash": "^4.17.21",
    "openai": "^3.1.0",
    "replace": "^1.2.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.19",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.11",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "bin": "oregen",
    "dirname": "oregen",
    "commands": "./dist/commands",
    "hooks": {
      "prerun": "./dist/hooks/prerun/cfonts"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " ",
    "topics": {
      "generate": {
        "description": "List of supported generated contents"
      },
      "generate:component": {
        "description": "Command to generate components"
      },
      "generate:hook": {
        "description": "Command to generate custom hooks"
      },
      "generate:view": {
        "description": "Command to generate views, view model, and route"
      },
      "generate:store": {
        "description": "Command to generate redux store"
      },
      "scaffold": {
        "description": "Command to scaffold/init a new project"
      }
    }
  }
}